# Environment Variables Example
# Copy this file to .env and fill in your values

# JWT Secret (generate a strong random string for production)
JWT_SECRET_KEY=change_this_secret_in_production

# GitHub OAuth (optional - for GitHub sign-in)
# Create an OAuth app at: https://github.com/settings/developers
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_REDIRECT_URI=http://localhost/api/auth/github/callback

# Frontend URL (used for OAuth redirects)
FRONTEND_URL=http://localhost

# Backend URL (used by frontend to call APIs)
REACT_APP_BACKEND_URL=http://localhost
